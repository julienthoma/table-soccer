{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": [
    "jasmine"
  ],
  "rules": {
    "linebreak-style": "off",
    "eol-last": 1,
    "max-len": [
      "error",
      {
        "code": 80,
        "ignoreTemplateLiterals": false
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-mixed-operators": 0,
    "no-confusing-arrow": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "no-return-assign": 0,
    "no-case-declarations": 0,
    "consistent-return": 0,
    "arrow-parens": 0,
    "space-before-function-paren": 0,
    "react/jsx-indent": [
      "error",
      2
    ],
    "react/no-array-index-key": 0,
    "import/prefer-default-export": 0,
    "react/jsx-indent-props": [
      "error",
      2
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/self-closing-comp": [
      "error",
      {
        "html": false
      }
    ],
    "jsx-a11y/no-static-element-interactions": 0
  },
  "env": {
    "browser": true,
    "jasmine": true
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
