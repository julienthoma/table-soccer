{
    "hosting": {
        "public": "public",
        "rewrites": [{
            "source": "**",
            "destination": "/index.html"
        }],
        "headers": {
            "source": "/app.js",
            "headers": [{
                "key": "Cache-Control",
                "value": "max-age=7200"
            }]
        }
    }
}
